{
  "name": "@solid-soda/voters",
  "version": "1.0.3",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "ci": "yarn s lint && yarn types",
    "prepare": "yarn build",
    "types": "tsc --noEmit",
    "s": "yarn soda",
    "postinstall": "yarn soda post-install"
  },
  "dependencies": {},
  "devDependencies": {
    "@solid-soda/scripts": "^1.1.1",
    "rimraf": "^2.6.2",
    "typescript": "^3.2.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn soda lint-staged && yarn types",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS -x '@commitlint/config-conventional'"
    }
  }
}
